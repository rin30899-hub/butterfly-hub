<!doctype html>
<html>
<head>
  <meta charset="utf-8"><meta name="viewport" content="width=device-width, initial-scale=1">
  <title><%= title %> â€“ <%= siteTitle %></title>
  <link href="https://cdn.jsdelivr.net/npm/bootstrap@5.3.0/dist/css/bootstrap.min.css" rel="stylesheet">
</head>
<body>
  <%- include('partials/navbar') %>
  <main class="container py-4">
    <h2 class="mb-3">Feedback Saved</h2>
    <div class="table-responsive">
      <table class="table table-striped align-middle">
        <thead><tr>
          <th>ID</th><th>Name</th><th>Item</th><th>Email</th>
          <th>Date (ISO)</th><th>Rating</th><th>Message</th>
        </tr></thead>
        <tbody>
          <% rows.forEach(r => { %>
            <tr>
              <td><%= r.id %></td>
              <td><%= r.name %></td>
              <td><%= r.item %></td>
              <td><%= r.email %></td>
              <td><%= r.date %></td>
              <td><%= r.rating %></td>
              <td><%= r.message %></td>
            </tr>
          <% }) %>
        </tbody>
      </table>
    </div>
    <a class="btn btn-primary" href="/feedback">Add another</a>
  </main>
</body>
</html>
